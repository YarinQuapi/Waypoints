# This workflow is used to automatically build the QWaypoints plugin.

name: QWaypoints Build

on:
  push:
    branches: [ $default-branch, 4.1 ]
  pull_request:
    branches: [ $default-branch, 4.1 ]

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v3
      - name: Set up JDK 17
        uses: actions/setup-java@v3
        with:
          java-version: '17'
          distribution: 'temurin'
          cache: maven
      - name: Build with Maven
        run: mvn clean install -DskipTests